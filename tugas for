#include <iostream>
using namespace std;

//TUGAS FOR FATIH

int main () {
	for (int m = 100; m <= 1000; m++)
	{
		if (m % 2==0)
		{
			cout << m << "ADALAH BILANAGN GEANAP "<<endl;
		}
	}
	
	
		for (int n = 100; n <= 1000; n++)
	{
		if (n % 2==1)
		{
			cout << n << "ADALAH BILANAGN GANJIL "<<endl;
		}
	}
	
